
<div class="col-md-10 white-background" style="padding-bottom:20%">
  <div class="row">
    <div class="col-md-6">
      <h2>Admins</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Last Sign-in</th>
            <th>Admin?</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <%= render @admins, partial: 'user' %>
        </tbody>
      </table>
    </div>
    <div class="col-md-6">
      <h2>Users</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Last Sign-in</th>
            <th>Admin?</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <%= render @users %>
        </tbody>
      </table>
      <%= paginate @users %>
    </div>
  </div>
</div>

