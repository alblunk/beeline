<!-- HEADER STARTS -->
      <div class="col-md-2" >
        <div class="row">
          <div class="col-md-12">
            <div id="header">
              <div class="logo centerize">
                <a href="#"><%= image_tag("FarallonCo-LongForm-T.png", width: "150") %></a>
              </div>
              <hr style="margin:0 0 -10px 0;">
              <ul class="social centerize">
                <li><a style="margin-right:18px" href="http://facebook.com/farallonco"><%= image_tag("facebook-w.png", width: "20") %></a>
                <a style="margin-right:18px" href="http://twitter.com/farallonco"><%= image_tag("twitter-w.png", width: "20") %></a><a style="margin-right:18px" href="http://instagram.com/farallonco"><%= image_tag("instagram.png", width: "18") %></a></li>
              </ul>
              <hr style="margin:-5px 0 0 0;">
              <div class="signup centerize">
                <a data-toggle="modal" href="#joinus-modal">&nbsp;&nbsp;&nbsp;JOIN US</a>
                <span id="line"></span>
                <a data-toggle="modal" href="#signin-modal">&nbsp;SIGN IN</a>

                <!-- <a data-toggle="modal" href="#joinus-modal">&nbsp;&nbsp;&nbsp;ACCOUNT</a>
                <span id="line"></span>
                <a data-toggle="modal" href="#signin-modal">&nbsp;LOG OUT</a> -->
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6 col-xs-5">
            <button type="button" id="emailus">
              <a href="mailto:support@farallon.co"><span class="glyphicon glyphicon-send"></span>&nbsp;Email us.</a>
            </button>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6 col-xs-5">
            <a data-toggle="modal" href="#submit-modal" >
              <div id="submit">
                <h5>Have an idea?</h5>
              </div>
            </a>
          </div>
        </div>
      </div>
